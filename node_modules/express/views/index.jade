doctype html
html
  head
    title UMBC HACKS
    
    link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css', integrity='sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7', crossorigin='anonymous')
    
    script(src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js', integrity='sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS', crossorigin='anonymous')
    
    link(href="https://fonts.googleapis.com/css?family=Droid+Serif" rel="stylesheet" type="text/css")

    link(href='https://fonts.googleapis.com/css?family=Ubuntu', rel='stylesheet', type='text/css')

    style
      include ../stylesheets/myStyle.css
  body
    script(src='https://ajax.googleapis.com/ajax/libs/angularjs/1.5.0/angular.min.js')
    script(src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js', integrity='sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS', crossorigin='anonymous')
    nav.navbar.navbar-default.navbar-static-top

      .container.navbarBackground
        .navbar-header
          button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='#') Political Tracker
        #navbar.navbar-collapse.collapse
          ul.nav.navbar-nav.navbar-right
            li.active
              a(href='#') Home
            li
              a(href='#about') About
            li.dropdown
              a.dropdown-toggle(href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false')
                | Candidates 
                span.caret
                ul.dropdown-menu
                  li.dropdown-header Republicans
                  li
                    a(href='#') Donald Trump
                  li
                    a(href='#') Ted Cruz
                  li
                    a(href='#') Marco Rubio
                  li.divider(role='separator')
                  li.dropdown-header Democrats
                  li
                    a(href='#') Hilary Clinton
                  li
                    a(href='#') Bernie Sanders
        // /.nav-collapse
    .container    
      .row
        .col-md-12
          .header
            h1.headline.text-center Political Tracker
      .row
        .col-md-12
          p.bodyText.text-center Choose a candidate 
      br
      br
      .row
        .col-xs-4
          img.candidateImage.img-responsive.republicanBorder.pull-left(src='http://cdn1.theodysseyonline.com/files/2016/02/17/635913406836700968-2122015760_doald.jpg')

        .col-xs-4
          img.candidateImage.img-responsive.republicanBorder.center-block(src='http://blogs.telegraph.co.uk/news/files/2010/10/rubio_1626005c.jpg')

        .col-xs-4
          img.candidateImage.img-responsive.republicanBorder.pull-right(src = 'http://i.imgur.com/QPwvole.png?1')

      br

      .row.secondRow
        .col-xs-4
          img.candidateImage.img-responsive.democratBorder.pull-left(src='https://www.247famous.com/wp-content/uploads/2015/10/bernie_sanders_pointing-oil-painting-portrait-e1445245259961.jpg')

        .col-xs-4
          img.candidateImage.img-responsive.center-block(src='http://classacttech.com/logo.png') 

        .col-xs-4
          img.candidateImage.img-responsive.democratBorder.pull-right(src='http://www.southdacola.com/blog/wp-content/uploads/2009/08/hillary_clinton.jpg')


